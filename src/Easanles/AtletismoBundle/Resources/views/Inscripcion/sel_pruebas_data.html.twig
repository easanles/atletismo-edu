            <tbody>
               <tr>
                  <th>Prueba</th>
                  <th class="hidden-xs">Precio</th>
                  <th><abbr title="Selección" class="visible-xs-block">Selec.</abbr><div class="hidden-xs">Selección</div></th>
               </tr>
               {% for pru in listaPru %}
                  {% if pru.sidTprm.sexo == 0 %}
                     {% set tpr = pru.sidTprm.sidTprf.nombre ~ ', masculino' %}
                  {% elseif pru.sidTprm.sexo == 1 %}
                     {% set tpr = pru.sidTprm.sidTprf.nombre ~ ', femenino' %}
                  {% elseif pru.sidTprm.sexo == 2 %}
                     {% set tpr = pru.sidTprm.sidTprf.nombre %}
                  {% endif %}
                  {% if entornos|length > 1 %}
                     {% set tpr = tpr ~ '. ' ~ pru.sidTprm.entorno %}
                  {% endif %}
                  <tr>
                     <td>{{ tpr }}</td>
                     <td class="hidden-xs">{{ pru.coste|number_format(2, ',', '.') }}€</td>
                     <td><button id="sel-pru-{{ pru.sid }}" type="checkbox" data-toggle="button" class="btn btn-xs btn-block btn-default sel-btn" onClick="toggleCheckButton(this)"><span class="glyphicon glyphicon-unchecked"></span> NO</button></td>
                  </tr>
               {% endfor %}
            </tbody>

      